<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ke.co.ewaste.rstar4">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ke.co.ewaste.rstar4.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ke.co.ewaste.rstar4.SearchActivity" android:label="Search"></activity>
        <meta-data
            android:name="AA_DB_NAME"
            android:value="ewaste.db" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />
        <meta-data
            android:name="AA_MODELS"
            android:value="ke.co.ewaste.rstar4.db.WasteDb" />
    </application>

</manifest>
